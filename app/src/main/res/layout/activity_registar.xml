<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistarActivity">

    <TextView
        android:id="@+id/textViewTesteDB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TESTE DB"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonShowRecords"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Show Records"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTesteDB" />

    <TextView
        android:id="@+id/textViewNoError"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:text="No Error"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonShowRecords" />

    <Button
        android:id="@+id/buttonBD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/textViewTesteDB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewTesteMD5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TESTE MD5"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonShowRecords"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTesteDB" />

    <TextView
        android:id="@+id/textViewTesteIguais"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/buttonShowRecords"
        app:layout_constraintTop_toBottomOf="@+id/textViewTesteMD5"
        tools:layout_editor_absoluteX="176dp" />
</androidx.constraintlayout.widget.ConstraintLayout>